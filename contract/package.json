{
  "name": "poco",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "compile": "pnpm exec truffle compile && pnpm run types",
    "types": "pnpm exec typechain --target=truffle-v5 'build/contracts/*.json'",
    "migrate": "pnpm exec truffle migrate",
    "remigrate": "pnpm exec truffle migrate -f 1",
    "repl": "npnpm execpx truffle console",
    "clean": "rm -rf build types",
    "test": "pnpm exec truffle test",
    "postinstall": "pnpm run compile"
  },
  "devDependencies": {
    "@typechain/truffle-v5": "^8.0.1",
    "@types/chai": "^4.3.3",
    "chai": "^4.3.6",
    "chalk": "^5.0.1",
    "truffle": "^5.5.30",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.7.3",
    "@openzeppelin/contracts-upgradeable": "^4.7.3",
    "@openzeppelin/truffle-upgrades": "^1.16.0"
  }
}
