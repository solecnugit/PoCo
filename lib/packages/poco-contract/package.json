{
  "name": "@poco/contract",
  "version": "0.0.1",
  "description": "",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "compile": "rollup --config rollup.config.ts",
    "setup": "./setup.sh",
    "types": "pnpm exec typechain --target=truffle-v5 'build/contracts/*.json' && pnpm exec typechain --target=ethers-v5 'build/contracts/*.json'",
    "migrate": "pnpm exec truffle migrate",
    "remigrate": "pnpm exec truffle migrate --reset",
    "repl": "pnpm exec truffle console",
    "clean": "rm -rf build types",
    "test": "pnpm exec truffle test",
    "ganache": "./ganache.sh",
    "build": "pnpm exec truffle compile && pnpm run types && pnpm run setup && pnpm run compile",
    "postinstall": "pnpm run build"
  },
  "devDependencies": {
    "@openzeppelin/contracts": "^4.7.3",
    "@openzeppelin/contracts-upgradeable": "^4.7.3",
    "@openzeppelin/truffle-upgrades": "^1.16.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/truffle-v5": "^8.0.1",
    "@types/bn.js": "^5.1.1",
    "@types/chai": "^4.3.3",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.18",
    "bn.js": "^5.2.1",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-bn": "^0.3.1",
    "chalk": "4.1.2",
    "ganache": "^7.4.3",
    "truffle": "^5.5.30",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "^4.8.3",
    "web3": "^1.8.0",
    "web3-eth": "^1.8.0"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.1",
    "ethers": "^5.7.1"
  }
}
